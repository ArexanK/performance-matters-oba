<%- include('./partials/head.ejs') %>
<%- include('./partials/header.ejs') %>

<section>
  <h2> Activiteiten</h2>

  <div class="wrapper">
      <i class=" arrow-carousel material-symbols-outlined">
          chevron_left
      </i>

      <ul class="carousel">
          <% results.forEach(result => { %>
          <li class="book">
              <a href="/item?id=<%= result.id %>" class="book-a">
                  <picture class="book-picture">
                      <img src="<%= result.coverimages %> " alt="<%= result.titles[0] %>">
                  </picture>
              </a>
          </li>
          <% }); %>
      </ul>
      
      <i class=" arrow-carousel material-symbols-outlined">
          chevron_right
      </i>
  </div>
</section>

<article>
   <h2>Cinema Literair</h2>
    <picture>
        <source srcset="/assets/cinema-literiair-activiteit-foto.webp" type="image/webp"/>
        <img src="/assets/cinema-literiair-logo.png.rendition.792.1267.png" alt="cinema activiteit" width="500px" height="400px" class="right">
    </picture>



<h2>OBA helpt</h2>
    <picture>
        <source srcset="/assets/foto-activiteit.rendition.792.1267.webp" type="image/webp"/>
        <img src="/assets/0818_leef-en-leerboudewijnbollmannobataalcafe.jpg.rendition.jpeg" alt="activiteit"width="500px" height="400px" class="left">
    </picture>


    
</article>



